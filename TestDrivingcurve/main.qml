    property var drivLine: []

    property var currentIndex: 0

    property var mbearing: 0

    title: qsTr("Map")

    Button {
        id: goStart
        z: drivingMap.z +1
        text: qsTr("开始")
        onClicked: {
            drivingMap.bearing = 0;
            timeStart.start()
            timeBack.stop()
        }
    }

    Button {
        id: goBack
        z: drivingMap.z +1
        text: qsTr("返航")
        anchors.verticalCenter: goStart.verticalCenter
        anchors.left: goStart.right
        anchors.leftMargin:  115
        onClicked: {
            drivingMap.bearing = 0;
            timeStart.stop()
            timeBack.start()
            backLine.visible = true
        }
    }

    Plugin {
        id: mapPlugin
        name: "osm"
    }

    Map {
        id: drivingMap
        visible: true
        width: parent.width
        height: parent.height +20


        plugin: mapPlugin
        center:  QtPositioning.coordinate(line[currentIndex].x, line[currentIndex].y)
        color: "white"
        zoomLevel: 15


        MapQuickItem { //当前位置
            id: currentLoaction
            width: 22
            height: 18
            zoomLevel: 0
            coordinate: QtPositioning.coordinate(line[currentIndex].x, line[currentIndex].y)
            sourceItem: Image{
                id: locationImage
                source: "qrc:/location.png"
            }
            anchorPoint.x: 11
            anchorPoint.y: 9
            visible: true
            rotation: mbearing
        }


        // 绘制折线 前进曲线
        MapPolyline {
            id: startLine
            line.width: 2
            line.color: "red"
            //路径列表 path : list<coordinate>
            //            path:line //TODO

        }


        // 绘制折线 返回曲线
        MapPolyline {
            id: backLine
            visible: false
            line.width: 2
            line.color: "green"
            //路径列表 path : list<coordinate>
        }

        // 绘制折线 返回方向线
        MapPolyline {
            id: returndirline
            visible: false
            line.width: 2
            line.color: "yellow"
        }

    }


    function  getBackPath(){
        currentIndex--
        if (currentIndex == 0){
            currentIndex = line.length -1
            backLine.path = []
        }
        backLine.addCoordinate(QtPositioning.coordinate(line[currentIndex].x, line[currentIndex].y))
        var c1 = QtPositioning.coordinate(line[currentIndex].x, line[currentIndex].y)
        var c2 = QtPositioning.coordinate(line[currentIndex+1].x, line[currentIndex+1].y)
        mbearing = c2.azimuthTo(c1)
    }

    function  getGoPath() {
        currentIndex ++
        if (currentIndex >= line.length-1){
            startLine.path = []

            currentIndex = 0;

        }

        startLine.addCoordinate(QtPositioning.coordinate(line[currentIndex].x, line[currentIndex].y))
        var c1 = QtPositioning.coordinate(line[currentIndex].x, line[currentIndex].y)
        var c2 = QtPositioning.coordinate(line[currentIndex-1].x, line[currentIndex-1].y)
        mbearing = c2.azimuthTo(c1)

    }

    Timer {
        id: timeStart
        repeat: true
        interval: 100
        onTriggered: {
            getGoPath()
        }
        running: false
    }
    Timer {
        id: timeBack
        repeat: true
        interval: 100
        onTriggered: {
            getBackPath()
        }
        running: false
    }

}

